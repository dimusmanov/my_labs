import figures.circle as ci
import figures.triangle as tr
import figures.square as sq
cp = ci.circle_perimeter(7)
ca = ci.circle_area(3)
tp = tr.triangle_perimeter(5, 6, 7)
ta = tr.triangle_area(2,5,6)
sp = sq.square_perimeter(9)
sa = sq.square_area(4)
print(cp)
print(ca)
print(tp)
print(ta)
print(sp)
print(sa)
cp = ci.circle_perimeter()
ca = ci.circle_area()
tp = tr.triangle_perimeter()
ta = tr.triangle_area()
sp = sq.square_perimeter()
sa = sq.square_area()
print(cp)
print(ca)
print(tp)
print(ta)
print(sp)
print(sa)
